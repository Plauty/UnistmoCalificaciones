CREATE DEFINER=`root`@`localhost` PROCEDURE `UPDATE_ALUMNO`( IN IdAlumnoup  Varchar(45) , IN Nombreup  Varchar(45) , IN ApellidoPaternoup  Varchar(45) , IN ApellidoMaternoup  Varchar(45) , IN Telefonoup  Varchar(45) , IN Emailup  Varchar(45) , IN Licenciaturaup  Varchar(45) , IN Semestreup  Varchar(45) , IN Grupoup  Varchar(45) , IN Estadoup  Varchar(45) )
BEGIN  UPDATE alumnos SET idAlumno=IdAlumnoup ,nombre=Nombreup ,apellidoPaterno=ApellidoPaternoup ,apellidoMaterno=ApellidoMaternoup ,telefono=Telefonoup ,email=Emailup ,licenciatura=Licenciaturaup ,semestre=Semestreup ,grupo=Grupoup ,estado=Estadoup    WHERE idAlumno=IdAlumnoup ; END